{
  "defaultInterval": 10000,
  "defaultOutput": "stdout",
  "things": {
    "10.0.0.2": "airmax-ap"
  },
  "input": {
    "snmp": {
      "timeout": 9900
    },
    "edgerouter": {
      "community": "nycmesh"
    },
    "unifi-ap": {
      "community": "nycmesh"
    },
    "airmax-ap": {
      "community": "nycmesh"
    },
    "airfiber": {
      "community": "nycmesh"
    },
    "mikrotik-router": {
      "port": 8729,
      "username": "admin",
      "password": "xxx"
    }
  },
  "output": {
    "stdout": {
      "format": "json"
    },
    "influxdb": {
      "db": "xxx",
      "username": "admin",
      "password": "xxx",
      "protocol": "http",
      "host": "10.0.0.1",
      "port": 8086
    },
    "edgerouter": {
      "tag": [
        "sysName",
        "ipAddress"
      ],
      "nested": {
        "interface": {
          "inherit": "sysName",
          "tag": [
            "sysName",
            "ifDescr",
            "ifPhysAddress"
          ]
        },
        "peer": {
          "inherit": "sysName",
          "tag": [
            "sysName",
            "remoteAs",
            "remoteId"
          ]
        },
        "route": {
          "inherit": "sysName",
          "tag": [
            "sysName",
            "destination",
            "nexthop"
          ]
        },
        "cpu": {
          "inherit": "sysName",
          "tag": [
            "sysName",
            "index"
          ]
        },
        "storage": {
          "inherit": "sysName",
          "tag": [
            "sysName",
            "hrStorageDescr"
          ]
        }
      }
    },
    "unifi-ap": {
      "tag": [
        "sysName",
        "ipAddress"
      ],
      "nested": {
        "interface": {
          "inherit": "sysName",
          "tag": [
            "sysName",
            "ifDescr",
            "ifPhysAddress"
          ]
        },
        "unifi-radio": {
          "inherit": "sysName",
          "alias": {
            "unifiRadioRadio": "unifiRadio"
          },
          "tag": [
            "sysName",
            "unifiRadio"
          ]
        },
        "unifi-wlan": {
          "inherit": "sysName",
          "alias": {
            "unifiVapRadio": "unifiRadio"
          },
          "tag": [
            "sysName",
            "unifiRadio"
          ]
        }
      }
    },
    "airmax-ap": {
      "tag": [
        "airmaxAp",
        "ipAddress"
      ],
      "nested": {
        "interface": {
          "inherit": "sysName",
          "tag": [
            "sysName",
            "ifDescr",
            "ifPhysAddress"
          ]
        },
        "airmax-wlan": {
          "inherit": "sysName",
          "alias": {
            "ubntWlStatSsid": "wlan"
          },
          "tag": [
            "sysName",
            "wlan"
          ]
        },
        "airmax-station": {
          "inherit": "sysName",
          "alias": {
            "ubntWlStatSsid": "wlan"
          },
          "tag": [
            "sysName",
            "ubntStaName",
            "ubntStaMac",
            "wlan"
          ]
        }
      }
    },
    "airfiber": {
      "tag": "sysName"
    },
    "mikrotik-router": {
      "tag": "sysName",
      "nested": {
        "route": {
          "inherit": "sysName",
          "tag": [
            "sysName",
            "gateway"
          ]
        },
        "peer": {
          "inherit": "sysName",
          "tag": [
            "sysName",
            "remoteAs",
            "remoteId",
            "name"
          ]
        },
        "interface": {
          "inherit": "sysName",
          "tag": [
            "sysName",
            "name"
          ]
        }
      }
    }
  }
}
